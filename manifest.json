{
  "manifest_version": 3,
  "name": "Camera Popup Extension",
  "version": "1.0",
  "description": "A simple extension to open the camera in a popup.",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
